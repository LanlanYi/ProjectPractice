<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE hibernate-mapping PUBLIC 
    "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
    "http://www.hibernate.org/dtd/hibernate-mapping-3.0.dtd">
<hibernate-mapping>

	<class name="edu.tsinghua.entity.UserLoginInfo" table="userLoginInfo">
		<id name="loginId" type="int" column="loginId">
			<generator class="increment"></generator>
		</id>
		<property name="userName" type="java.lang.String"></property>
		<property name="userPwd" type="java.lang.String"></property>
		
	</class>

</hibernate-mapping>
    