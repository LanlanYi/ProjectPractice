<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE hibernate-mapping PUBLIC 
    "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
    "http://www.hibernate.org/dtd/hibernate-mapping-3.0.dtd">
<hibernate-mapping>

	<class name="edu.tsinghua.entity.ZhangMuInfo" table="zhangMuInfo">
		<id name="zhangmuId" type="int" column="zhangmuId">
			<generator class="increment"></generator>
		</id>
		<property name="kuaijiId" type="int"></property>
		<property name="time" type="java.lang.String"></property>
		<property name="jizhangId" type="int"></property>
		<property name="amount" type="double"></property>
		<property name="jieyu" type="double"></property>
		<property name="reply" type="java.lang.String"></property>
	
	</class>

</hibernate-mapping>
    