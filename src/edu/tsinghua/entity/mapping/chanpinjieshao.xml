<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE hibernate-mapping PUBLIC 
    "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
    "http://www.hibernate.org/dtd/hibernate-mapping-3.0.dtd">
<hibernate-mapping>

	<class name="edu.tsinghua.entity.ChanPinJieShao" table="chanPinJieShao">
		<id name="chanpinId" type="int" column="chanpinId">
			<generator class="increment"></generator>
		</id>
		<property name="chanPinTime" type="java.lang.String"></property>
		<property name="chanPinName" type="java.lang.String"></property>
		<property name="chanPinMiaoShu" type="java.lang.String"></property>
		
	</class>

</hibernate-mapping>
    