<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE hibernate-mapping PUBLIC 
    "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
    "http://www.hibernate.org/dtd/hibernate-mapping-3.0.dtd">
<hibernate-mapping>

	<class name="edu.tsinghua.entity.KaoQinSum" table="kaoQinSum">
		<id name="kaoQinSumId" type="int" column="kaoQinSumId">
			<generator class="increment"></generator>
		</id>
		<property name="month" type="java.lang.String"></property>
		<property name="departId" type="int"></property>
		<property name="yuangongNo" type="java.lang.String"></property>
		<property name="supposeChuqin" type="int"></property>
		<property name="actualChuqin" type="int"></property>
		<property name="queqin" type="int"></property>
		<property name="overtime" type="int"></property>
		
	</class>

</hibernate-mapping>
    