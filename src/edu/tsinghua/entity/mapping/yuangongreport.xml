<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE hibernate-mapping PUBLIC 
    "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
    "http://www.hibernate.org/dtd/hibernate-mapping-3.0.dtd">
<hibernate-mapping>

	<class name="edu.tsinghua.entity.YuanGongReport" table="yuanGongReport">
		<id name="yuangongReportId" type="int" column="yuangongReportId">
			<generator class="increment"></generator>
		</id>
		<property name="yuangongId" type="int"></property>
		<property name="jingliId" type="int"></property>
		<property name="content" type="java.lang.String"></property>
		<property name="reply" type="java.lang.String"></property>
		<property name="status" type="java.lang.String"></property>
		
	</class>

</hibernate-mapping>
    